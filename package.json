{
  "name": "btms-portal-frontend",
  "version": "0.0.0",
  "description": "BTMS Frontend",
  "sideEffects": false,
  "main": ".server/index.js",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "run-s build:frontend",
    "build:frontend": "NODE_ENV=production webpack",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "frontend:watch": "NODE_ENV=development webpack --watch",
    "git:pre-commit-hook": "npm run test:lint && npm test",
    "postinstall": "npm run setup:husky && npm run build",
    "lint": "run-s test:lint lint:scss",
    "lint:fix": "eslint --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "postversion": "git add package.json package-lock.json && git commit -m $npm_package_version",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:lint": "eslint",
    "server:watch": "node --inspect=0.0.0.0 --watch --watch-path=src --env-file-if-exists=.env.local src/index.js",
    "server:debug": "node --inspect-brk=0.0.0.0 --watch --env-file-if-exists=.env.local src/index.js",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@defra/hapi-tracing": "^1.26.0",
    "@elastic/ecs-pino-format": "^1.5.0",
    "@hapi/bell": "^13.1.0",
    "@hapi/catbox-memory": "^6.0.2",
    "@hapi/catbox-redis": "^7.0.2",
    "@hapi/cookie": "^12.0.1",
    "@hapi/hapi": "^21.4.3",
    "@hapi/inert": "^7.1.0",
    "@hapi/jwt": "^3.2.0",
    "@hapi/vision": "^7.0.3",
    "@hapi/yar": "^11.0.3",
    "aws-embedded-metrics": "^4.2.0",
    "chart.js": "4.5.0",
    "convict": "^6.2.4",
    "cssnano": "^7.1.1",
    "cssnano-preset-default": "^7.0.6",
    "date-fns": "^4.1.0",
    "govuk-frontend": "5.11.2",
    "hapi-pino": "^12.1.0",
    "hapi-pulse": "^3.0.1",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.7.0",
    "joi": "^18.0.1",
    "nunjucks": "^3.2.4",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "30.0.5",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "5.0.2",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "cheerio": "^1.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "9.34.0",
    "global-jsdom": "26.0.0",
    "husky": "^9.1.7",
    "jest": "30.0.5",
    "jest-environment-jsdom": "30.0.5",
    "jest-fetch-mock": "3.0.3",
    "neostandard": "0.12.2",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "sass-embedded": "^1.90.0",
    "sass-loader": "^16.0.5",
    "source-map-loader": "^5.0.0",
    "stylelint": "^16.23.1",
    "stylelint-config-gds": "^2.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.101.3",
    "webpack-assets-manifest": "^5.2.1",
    "webpack-cli": "^6.0.1"
  }
}
