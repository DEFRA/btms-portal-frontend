{% if accountNavigation | length > 0 %}
  <ul class="govuk-service-navigation__list govuk-service-navigation__list--right">
    {% for link in accountNavigation %}
      <li class="govuk-service-navigation__item">
        <a class="govuk-service-navigation__link" href="{{ link.href }}">{{ link.text }}</a>
      </li>
    {% endfor %}
  </ul>
{% endif %}
