{% from "govuk/components/tag/macro.njk" import govukTag %}

{% macro decisionMatch(match) %}
  {% if match === true %}
    {{ govukTag({ text: "Yes", classes: "govuk-tag--green" }) }}
  {% elif match === false %}
    {{ govukTag({ text: "No", classes: "govuk-tag--red" }) }}
  {% endif %}
{% endmacro %}
